<div class="row">
  <div class="col">
    <%= form_with(model: @merchant, url: admin_merchant_path(@merchant.id) , method: :patch, local: true) do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div>
      <%= f.submit 'update merchant' %>
    </div>
    <% end %>
  </div>
</div>